{
  "name": "supadupacode",
  "version": "1.0.0",
  "description": "CLI Orchestrator for multi-agent development automation",
  "main": "dist/src/index.js",
  "bin": {
    "supadupacode": "./dist/src/index.js",
    "sd": "./bin/sd.js"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch & node dist/index.js",
    "start": "node dist/index.js",
    "start:dev": "ts-node src/index.ts",
    "test": "npm run build && node --test tests/**/*.test.js",
    "test:watch": "npm run build:watch & node --test --watch tests/**/*.test.js",
    "test:ts": "ts-node node_modules/.bin/tap tests/**/*.test.ts",
    "lint": "eslint src/**/*.ts tests/**/*.ts --fix",
    "lint:check": "eslint src/**/*.ts tests/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "pretest": "npm run type-check && npm run lint:check"
  },
  "keywords": [
    "cli",
    "orchestrator",
    "automation",
    "ai-agents",
    "mcp"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cli-progress": "^3.11.6",
    "ajv": "^8.17.1",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "commander": "^11.0.0",
    "inquirer": "^9.2.0",
    "node-fetch": "^2.7.0",
    "ora": "^7.0.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@types/sqlite3": "^3.1.11",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
